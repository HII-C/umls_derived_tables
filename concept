# POPULATE DERIVED.CONCEPTS WITH CONCEPTS FROM UMLS
SELECT * FROM derived.concept LIMIT 100;
INSERT INTO derived.concept (cui, str)
  SELECT DISTINCT
    CUI,
    STR
  FROM umls.MRCONSO
  WHERE CUI IN (
    SELECT DISTINCT CUI
    FROM umls.MRCONSO
    WHERE SAB IN ("RXNORM", "ICD10CM", "ICD9CM", "SNOMEDCT_US", "CPT", "LNC", "NDFRT")
  ) AND LENGTH(STR) <= 255 AND ISPREF = 'Y' AND LAT = 'ENG' AND STT = 'PF' AND TS = 'P';
